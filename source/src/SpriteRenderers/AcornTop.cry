
function SPRITE_renderAcornTop(sprite, scene, offsetX, offsetY, arc) {
	left = sprite.x + offsetX - 8;
	top = sprite.y + offsetY - 8;
	width = 16;
	height = 16;
	path = 'acorn_projectile_1';
	path = 'sprites/' + path + '.png';
	img = IMAGES.get(path);
	$gfx_blit_image(img, left, top);
}
